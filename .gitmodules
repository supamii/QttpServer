[submodule "lib/libuv"]
	path = lib/libuv
	url = https://github.com/libuv/libuv.git
[submodule "lib/http-parser"]
	path = lib/http-parser
	url = https://github.com/nodejs/http-parser.git
[submodule "lib/mongo-cxx-driver"]
	path = lib/mongo-cxx-driver
	url = https://github.com/mongodb/mongo-cxx-driver.git
	ignore = dirty
[submodule "lib/qredisclient"]
	path = lib/qredisclient
	url = https://github.com/uglide/qredisclient.git
[submodule "lib/open-ssl"]
	path = lib/open-ssl
	url = https://github.com/openssl/openssl.git
